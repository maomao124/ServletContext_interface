<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         metadata-complete="false" version="4.0">
    <!-- metadata-complete取值为true，表示关闭注解支持 -->
    <!-- metadata-complete取值为false，表示启用注解支持 -->
    <display-name>servletDemo</display-name>
    <servlet>
        <!--指定 Servlet 的名称-->
        <servlet-name>ReadContextServlet</servlet-name>
        <!--Servlet 的完整限定名（包名+类名）-->
        <servlet-class>mao.servletcontext_interface.ReadContextServlet</servlet-class>
    </servlet>
    <!--定义 Servlet 与 URL 之间的映射-->
    <servlet-mapping>
        <!-- Servlet 的名称和虚拟路径-->
        <servlet-name>ReadContextServlet</servlet-name>
        <url-pattern>/ReadContextServlet</url-pattern>
    </servlet-mapping>

    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名1</param-name>
        <!--参数值-->
        <param-value>参数值1</param-value>
    </context-param>
    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名2</param-name>
        <!--参数值-->
        <param-value>参数值2</param-value>
    </context-param>
    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名4</param-name>
        <!--参数值-->
        <param-value>参数值4</param-value>
    </context-param>
    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名5</param-name>
        <!--参数值-->
        <param-value>参数值5</param-value>
    </context-param>
    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名6</param-name>
        <!--参数值-->
        <param-value>参数值6</param-value>
    </context-param>
    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名7</param-name>
        <!--参数值-->
        <param-value>参数值7</param-value>
    </context-param>
    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名8</param-name>
        <!--参数值-->
        <param-value>参数值8</param-value>
    </context-param>
    <!--设置全局初始化参数 -->
    <context-param>
        <!--参数名-->
        <param-name>参数名9</param-name>
        <!--参数值-->
        <param-value>参数值9</param-value>
    </context-param>

</web-app>